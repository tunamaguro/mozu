{
    "version": "2",
    "plugins": [
        {
            "name": "rust-postgres",
            "wasm": {
                "url": "https://github.com/tunamaguro/sqlc-rust-postgres/releases/download/v0.1.3/sqlc-rust-postgres.wasm",
                "sha256": "30abc75abe35a426ab63c207fe4f94344375f64f0acf163828b4b6f2a491eb1a"
            }
        }
    ],
    "sql": [
        {
            "schema": "src/infrastructure/migrations",
            "queries": "src/infrastructure/queries",
            "engine": "postgresql",
            "codegen": [
                {
                    "out": "src/infrastructure",
                    "plugin": "rust-postgres",
                    "options": {
                        "db_crate": "tokio_postgres",
                        "enum_derives": [
                            "PartialEq"
                        ],
                        "row_derives": [
                            "PartialEq"
                        ],
                        "overrides": [
                            {
                                "db_type": "uuid",
                                "rs_type": "uuid::Uuid"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}